<?xml version="1.0" encoding="UTF-8"?>
<table>	
	<!-- 是否全部生成,默认true -->
	<all>true</all>
	<ids>	
		<!-- 主键列名，默认为id -->
		<id name='id'></id>
	</ids>
	<columns>
		<column name='pay_date'>
			<!-- 是否检查非空，默认false -->
			<checkEmpty>false</checkEmpty>
			<!-- 如果不为空，按该表达式进行验证(js),默认空,注意:\要转义，写成\\ -->
			<regexJs>\\d{4}-\\d{2}-\\d{2}</regexJs>
			<!-- 如果不为空，按该表达式进行验证(java),默认空,注意:\要转义，写成\\ -->
			<regexJava>\\d{8}</regexJava>
			<!-- 是否为日期，默认false  -->
			<date>true</date>
			<!-- 是否可修改，默认true  -->
			<modify>false</modify>
			<!-- 查询方式{non:不是查询条件(默认),equals,like,before,after,range} -->
			<query>non</query>
		</column>
		<column name='record_name'>
			<!-- 是否检查非空，默认false -->
			<checkEmpty>false</checkEmpty>
			<!-- 如果不为空，按该表达式进行验证,默认空 -->
			<regex></regex>
			<!-- 是否为日期，默认false  -->
			<date>false</date>
			<!-- 是否可修改，默认true  -->
			<modify>false</modify>
			<!-- 查询方式{non:不是查询条件(默认),equals,like,before,after} -->
			<query>like</query>
		</column>
	</columns>
</table> 

